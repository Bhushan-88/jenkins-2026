pipeline {
    agent any
    stages {
        stage('pull') {
            steps {
                echo 'pull is successful '
            }
        }
        stage('build') {
            steps {
                echo 'congrates ! build is done'
            }
        }
        stage('test') {
            steps{
                echo 'test is success'
            }
        }
        stage('deploy') {
            steps{
                echo 'deploy is successful'
            }
        }
    }
}